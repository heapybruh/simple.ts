    "discordx": "^11.9.4",
    "dotenv": "^16.4.5",
    "moonlink.js": "github:heapybruh/moonlink.js"
  },
  "devDependencies": {
    "@types/node": "^20.14.2",
    "nodemon": "^3.1.3",
    "prettier": "^3.3.2",
    "ts-node": "^10.9.2",
    "typescript": "5.4.5"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  }
}
